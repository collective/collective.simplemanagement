<object xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        name="portal_actions"
        meta_type="Plone Actions Tool">
  <object name="user" meta_type="CMF Action Category">
    <object name="worklog" meta_type="CMF Action" insert-after="dashboard"
            i18n:domain="collective.simplemanagement">
      <property name="title" i18n:translate="">Worklog</property>
      <property name="description" i18n:translate=""/>
      <property name="url_expr">string:${portal_url}/@@worklog</property>
      <property name="link_target"/>
      <property name="icon_expr"/>
      <property name="available_expr"/>
      <property name="permissions">
        <element value="Simplemanagement: Manage project"/>
      </property>
      <property name="visible">True</property>
    </object>
    <object name="compass" meta_type="CMF Action" insert-after="worklog"
            i18n:domain="collective.simplemanagement">
      <property name="title" i18n:translate="">Compass</property>
      <property name="description" i18n:translate=""/>
      <property name="url_expr">string:${portal_url}/@@compass</property>
      <property name="link_target"/>
      <property name="icon_expr"/>
      <property name="available_expr"/>
      <property name="permissions">
        <element value="Simplemanagement: View compass"/>
      </property>
      <property name="visible">True</property>
    </object>
    <object name="bookings" meta_type="CMF Action" insert-after="compass"
            i18n:domain="collective.simplemanagement">
      <property name="title" i18n:translate="">Bookings</property>
      <property name="description" i18n:translate=""/>
      <property name="url_expr">string:${portal_url}/bookings</property>
      <property name="link_target"/>
      <property name="icon_expr"/>
      <property name="available_expr"/>
      <property name="permissions">
        <element value="Simplemanagement: Manage project"/>
      </property>
      <property name="visible">True</property>
    </object>
  </object>
  <object name="simplemanagement_actions" meta_type="CMF Action Category">
    <property name="title"></property>
    <object name="dashboard" meta_type="CMF Action" i18n:domain="plone">
     <property name="title" i18n:translate="">Dashboard</property>
     <property name="description" i18n:translate=""></property>
     <property name="url_expr">string:dashboard</property>
     <property name="link_target"></property>
     <property name="icon_expr"></property>
     <property name="available_expr"></property>
     <property name="permissions">
      <element value="Simplemanagement: Add booking"/>
      <element value="Simplemanagement: Manage project"/>
     </property>
     <property name="visible">True</property>
    </object>
    <object name="worklog" meta_type="CMF Action"
       i18n:domain="collective.simplemanagement">
     <property name="title" i18n:translate="">Worklog</property>
     <property name="description" i18n:translate=""></property>
     <property name="url_expr">string:worklog</property>
     <property name="link_target"></property>
     <property name="icon_expr"></property>
     <property name="available_expr"></property>
     <property name="permissions">
      <element value="Simplemanagement: Manage project"/>
     </property>
     <property name="visible">True</property>
    </object>
    <object name="planning" meta_type="CMF Action"
       i18n:domain="collective.simplemanagement">
     <property name="title" i18n:translate="">Planning</property>
     <property name="description" i18n:translate=""></property>
     <property name="url_expr">string:planning</property>
     <property name="link_target"></property>
     <property name="icon_expr"></property>
     <property name="available_expr"></property>
     <property name="permissions">
      <element value="Simplemanagement: Manage project"/>
     </property>
     <property name="visible">True</property>
    </object>
    <object name="bookings" meta_type="CMF Action"
       i18n:domain="collective.simplemanagement">
     <property name="title" i18n:translate="">Bookings</property>
     <property name="description" i18n:translate=""></property>
     <property name="url_expr">string:bookings</property>
     <property name="link_target"></property>
     <property name="icon_expr"></property>
     <property name="available_expr"></property>
     <property name="permissions">
      <element value="Simplemanagement: View booking"/>
     </property>
     <property name="visible">True</property>
    </object>
  </object>

</object>
